# Test pull extracts files correctly
gentag TAG
exec blob --plain-http push $REGISTRY/pull:$TAG sample-project

mkdir output
exec blob --plain-http pull $REGISTRY/pull:$TAG output
stdout 'Pulled'

exists output/README.md
exists output/config/app.yaml
exists output/src/main.go
exists output/docs/api/reference.md
cmp output/config/app.yaml sample-project/config/app.yaml
